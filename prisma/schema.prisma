generator client {
  provider        = "prisma-client-js"
  output          = "../src/generated/prisma"
  previewFeatures = ["postgresqlExtensions"]
}

datasource db {
  provider   = "postgresql"
  url        = env("DATABASE_URL")
  extensions = [postgis, uuid_ossp(map: "uuid-ossp", schema: "public")]
}

model addresses {
  id               Int                      @id @default(autoincrement())
  address_line     String?
  province         String?                  @db.VarChar(255)
  district         String?                  @db.VarChar(255)
  subdistrict      String?                  @db.VarChar(255)
  postal_code      String?                  @db.VarChar(20)
  location         Unsupported("geometry")?
  created_at       DateTime                 @default(now()) @db.Timestamptz(6)
  updated_at       DateTime                 @default(now()) @db.Timestamptz(6)
  air_quality      air_quality[]
  apartment        apartment[]
  emergency_calls  emergency_calls[]
  events           events[]
  facilities       facilities[]
  onsite_sessions  onsite_sessions[]
  user_profiles    user_profiles[]
  volunteer_events volunteer_events[]
  weather_data     weather_data[]
  weather_rating   weather_rating[]

  @@index([location], map: "idx_addresses_location", type: Gist)
}

model air_quality {
  id          Int                   @id @default(autoincrement())
  location_id Int?
  aqi         Decimal?              @db.Decimal(6, 2)
  pm25        Decimal?              @db.Decimal(8, 3)
  category    air_quality_category?
  measured_at DateTime              @default(now()) @db.Timestamptz(6)
  addresses   addresses?            @relation(fields: [location_id], references: [id], onUpdate: NoAction)
}

model alerts {
  id                Int                      @id @default(autoincrement())
  report_id         Int?
  user_id           Int?
  message           String
  status            alert_status?            @default(unread)
  location          Unsupported("geometry")?
  sent_at           DateTime?                @default(now()) @db.Timestamptz(6)
  emergency_reports emergency_reports?       @relation(fields: [report_id], references: [id], onDelete: Cascade, onUpdate: NoAction)
  users             users?                   @relation(fields: [user_id], references: [id], onDelete: Cascade, onUpdate: NoAction)
}

model ambulances {
  id               Int                      @id @default(autoincrement())
  vehicle_number   String?                  @unique @db.VarChar(50)
  status           String?                  @db.VarChar(50)
  current_location Unsupported("geometry")?
  base_facility_id Int?
  created_at       DateTime                 @default(now()) @db.Timestamptz(6)
  facilities       facilities?              @relation(fields: [base_facility_id], references: [id], onUpdate: NoAction)
  emergency_calls  emergency_calls[]
}

model apartment {
  id                 Int                 @id @default(autoincrement())
  name               String?             @db.VarChar(255)
  phone              String?             @db.VarChar(10)
  description        String?
  electric_price     Float?
  water_price        Float?
  internet           apartment_internet?
  apartment_type     apartment_type?
  apartment_location apartment_location?
  address_id         Int?
  internet_price     Decimal?            @db.Decimal(10, 2)
  addresses          addresses?          @relation(fields: [address_id], references: [id], onUpdate: NoAction)
}

model apartment_booking {
  id             Int             @id @default(autoincrement())
  user_id        Int
  check_in       DateTime?       @db.Timestamptz(6)
  booking_status booking_status? @default(pending)
  created_at     DateTime?       @default(now()) @db.Timestamptz(6)
  updated_at     DateTime?       @default(now()) @db.Timestamptz(6)
  room_id        Int?
  guest_name     String?         @db.VarChar(255)
  guest_phone    String?         @db.VarChar(10)
  guest_email    String?         @db.VarChar(255)
  room_type      String?         @db.VarChar(255)
  apartment_id   Int?
  users          users           @relation(fields: [user_id], references: [id], onDelete: SetNull, onUpdate: NoAction)
  room           room?           @relation(fields: [room_id], references: [id], onDelete: NoAction, onUpdate: NoAction, map: "fk_room_id")
}

model apartment_owner {
  id           Int @id @default(autoincrement())
  user_id      Int
  apartment_id Int
}

model apartment_picture {
  id           Int     @id @default(autoincrement())
  name         String? @db.VarChar(255)
  file_path    String
  apartment_id Int
}

model appointments {
  id               Int         @id @default(autoincrement())
  patient_id       Int?
  facility_id      Int?
  appointment_at   DateTime?   @db.Timestamptz(6)
  type             String?     @db.VarChar(50)
  created_at       DateTime    @default(now()) @db.Timestamptz(6)
  doctor_id        Int?
  consultation_fee Decimal?    @db.Decimal(12, 2)
  facilities       facilities? @relation(fields: [facility_id], references: [id], onUpdate: NoAction)
  patients         patients?   @relation(fields: [patient_id], references: [id], onUpdate: NoAction)
  doctors          doctors?    @relation(fields: [doctor_id], references: [id], onDelete: NoAction, onUpdate: NoAction, map: "fk_doctor_id")
}

model beds {
  id             Int         @id @default(autoincrement())
  facility_id    Int?
  bed_number     String?     @db.VarChar(50)
  bed_type       String?     @db.VarChar(50)
  status         String?     @db.VarChar(50)
  patient_id     Int?
  admission_date DateTime?   @db.Timestamptz(6)
  created_at     DateTime    @default(now()) @db.Timestamptz(6)
  facilities     facilities? @relation(fields: [facility_id], references: [id], onDelete: Cascade, onUpdate: NoAction)
  patients       patients?   @relation(fields: [patient_id], references: [id], onUpdate: NoAction)
}

model bin {
  id         Int       @id @default(autoincrement())
  bin_name   String?   @db.VarChar(255)
  bin_type   bin_type?
  latitude   Decimal?  @db.Decimal(10, 8)
  longitude  Decimal?  @db.Decimal(11, 8)
  address    String?
  capacity   Decimal?  @db.Decimal(10, 2)
  created_at DateTime  @default(now()) @db.Timestamptz(6)
  updated_at DateTime? @db.Timestamptz(6)
}

model card_transactions {
  id                   Int                    @id @default(autoincrement())
  card_id              Int
  card_type            String?                @db.VarChar(50)
  transaction_type     card_transaction_type?
  transaction_category transaction_category?
  reference            String?                @db.VarChar(100)
  amount               Decimal?               @default(0) @db.Decimal(12, 2)
  description          String?                @db.VarChar(255)
  created_at           DateTime               @default(now()) @db.Timestamptz(6)
}

model conversation_participants {
  conversation_id Int
  user_id         Int
  conversations   conversations @relation(fields: [conversation_id], references: [id], onDelete: Cascade, onUpdate: NoAction)
  users           users         @relation(fields: [user_id], references: [id], onDelete: Cascade, onUpdate: NoAction)

  @@id([conversation_id, user_id])
}

model conversations {
  id                        Int                         @id @default(autoincrement())
  conversation_name         String?                     @db.VarChar(255)
  created_at                DateTime                    @default(now()) @db.Timestamptz(6)
  conversation_participants conversation_participants[]
  messages                  messages[]
}

model course_videos {
  id                Int      @id @default(autoincrement())
  course_id         Int?
  video_name        String   @db.VarChar(255)
  video_description String?
  duration_minutes  Int
  video_order       Int
  video_file_path   String?
  created_at        DateTime @default(now()) @db.Timestamptz(6)
  updated_at        DateTime @default(now()) @db.Timestamptz(6)
  courses           courses? @relation(fields: [course_id], references: [id], onDelete: Cascade, onUpdate: NoAction)

  @@unique([course_id, video_order])
}

model courses {
  id                 Int               @id @default(autoincrement())
  author_id          Int?
  course_name        String            @db.VarChar(255)
  course_description String?
  course_type        course_type
  course_status      course_status     @default(pending)
  cover_image        String?
  created_at         DateTime          @default(now()) @db.Timestamptz(6)
  updated_at         DateTime          @default(now()) @db.Timestamptz(6)
  course_videos      course_videos[]
  users              users?            @relation(fields: [author_id], references: [id], onUpdate: NoAction)
  onsite_sessions    onsite_sessions[]
}

model departments {
  id                Int                 @id @default(autoincrement())
  department_name   String              @unique @db.VarChar(255)
  created_at        DateTime            @default(now()) @db.Timestamptz(6)
  updated_at        DateTime            @default(now()) @db.Timestamptz(6)
  events            events[]
  facilities        facilities[]
  freecycle_posts   freecycle_posts[]
  users_departments users_departments[]
  volunteer_events  volunteer_events[]
}

model digital_cards {
  id                  Int      @id @default(autoincrement())
  user_id             Int?
  status              String   @default("active") @db.VarChar(20)
  balance             Decimal? @db.Decimal(14, 2)
  finance_card_number String?  @unique @db.VarChar(100)
  users               users?   @relation(fields: [user_id], references: [id], onDelete: Cascade, onUpdate: NoAction)
}

model dim_category {
  category_id          Int                @id
  category_name        String?            @db.VarChar(255)
  category_description String?
  reports_metadata     reports_metadata[]
}

model dim_facility {
  facility_id     Int               @id
  facility_name   String?           @db.VarChar(255)
  location_id     Int?
  dim_location    dim_location?     @relation(fields: [location_id], references: [location_id], onDelete: NoAction, onUpdate: NoAction, map: "fk_location")
  fact_healthcare fact_healthcare[]
}

model dim_location {
  location_id     Int                      @id
  district        String?                  @db.VarChar(255)
  coordinates     Unsupported("geometry")?
  dim_facility    dim_facility[]
  fact_population fact_population[]
  fact_traffic    fact_traffic[]
  fact_waste      fact_waste[]
  fact_weather    fact_weather[]
}

model dim_time {
  time_id         Int               @id
  date_actual     DateTime?         @db.Date
  year_val        Int?
  month_val       Int?
  day_val         Int?
  hour_val        Int?
  fact_healthcare fact_healthcare[]
  fact_population fact_population[]
  fact_traffic    fact_traffic[]
  fact_waste      fact_waste[]
  fact_weather    fact_weather[]
}

model dim_user {
  user_id          Int                @id
  full_name        String?            @db.VarChar(255)
  email            String?            @unique @db.VarChar(255)
  role_string      String?            @db.VarChar(100)
  reports_metadata reports_metadata[]
}

model dim_waste_type {
  waste_type_id   Int          @id
  waste_type_name String?      @db.VarChar(255)
  fact_waste      fact_waste[]
}

model doctors {
  id               Int            @id @default(autoincrement())
  user_id          Int?
  specialization   String?        @db.VarChar(255)
  current_status   String?        @default("available") @db.VarChar(255)
  consultation_fee Decimal?       @default(0) @db.Decimal(12, 2)
  created_at       DateTime?      @default(now()) @db.Timestamptz(6)
  facility_id      Int?
  name             String?        @db.VarChar(255)
  appointments     appointments[]
  facilities       facilities?    @relation(fields: [facility_id], references: [id], onDelete: NoAction, onUpdate: NoAction, map: "fk_facility_id")
  users            users?         @relation(fields: [user_id], references: [id], onDelete: NoAction, onUpdate: NoAction, map: "fk_user_id")
}

model emergency_calls {
  id             Int         @id @default(autoincrement())
  patient_id     Int?
  caller_phone   String?     @db.VarChar(20)
  emergency_type String?     @db.VarChar(100)
  severity       String?     @db.VarChar(50)
  address_id     Int?
  ambulance_id   Int?
  facility_id    Int?
  status         String?     @db.VarChar(50)
  created_at     DateTime    @default(now()) @db.Timestamptz(6)
  addresses      addresses?  @relation(fields: [address_id], references: [id], onUpdate: NoAction)
  ambulances     ambulances? @relation(fields: [ambulance_id], references: [id], onUpdate: NoAction)
  facilities     facilities? @relation(fields: [facility_id], references: [id], onUpdate: NoAction)
  patients       patients?   @relation(fields: [patient_id], references: [id], onUpdate: NoAction)
}

model emergency_contacts {
  id           Int     @id @default(autoincrement())
  user_id      Int?
  contact_name String  @db.VarChar(255)
  phone        String? @db.VarChar(20)
  users        users?  @relation(fields: [user_id], references: [id], onDelete: Cascade, onUpdate: NoAction)
}

model emergency_reports {
  id                     Int            @id @default(autoincrement())
  user_id                Int?
  image_url              String?        @db.VarChar(1024)
  description            String?
  ambulance_service      Boolean?       @default(false)
  level                  report_level?
  status                 report_status? @default(pending)
  created_at             DateTime       @default(now()) @db.Timestamptz(6)
  updated_at             DateTime       @default(now()) @db.Timestamptz(6)
  title                  String
  contact_center_service Boolean?       @default(false)
  report_category        report_type?
  location               Unsupported("geometry")?
  lat                    String?
  long                   String?
  alerts                 alerts[]
  users                  users?         @relation(fields: [user_id], references: [id], onUpdate: NoAction)
}

model event_bookmarks {
  user_id    Int
  event_id   Int
  created_at DateTime @default(now()) @db.Timestamptz(6)
  events     events   @relation(fields: [event_id], references: [id], onDelete: Cascade, onUpdate: NoAction)
  users      users    @relation(fields: [user_id], references: [id], onDelete: Cascade, onUpdate: NoAction)

  @@id([user_id, event_id])
}

model event_organization {
  id           Int      @id @default(autoincrement())
  name         String?  @db.VarChar(255)
  email        String?  @db.VarChar(255)
  phone_number String?  @db.VarChar(255)
  events       events[]
}

model event_tag {
  event_id       Int             @id(map: "pk_event_tag")
  event_tag_id   Int?
  name           String?         @db.VarChar(255)
  events         events          @relation(fields: [event_id], references: [id], onDelete: Cascade, onUpdate: NoAction)
  event_tag_name event_tag_name? @relation(fields: [event_tag_id], references: [id], onDelete: Cascade, onUpdate: NoAction)
}

model event_tag_name {
  id        Int         @id @default(autoincrement())
  name      String?     @db.VarChar(255)
  event_tag event_tag[]
}

model events {
  id                     Int                      @id @default(autoincrement())
  host_user_id           Int?
  department_id          Int?
  image_url              String?
  title                  String?                  @db.VarChar(255)
  description            String?
  total_seats            Int?                     @default(0)
  start_at               DateTime?                @db.Timestamptz(6)
  end_at                 DateTime?                @db.Timestamptz(6)
  address_id             Int?
  created_at             DateTime                 @default(now()) @db.Timestamptz(6)
  updated_at             DateTime                 @default(now()) @db.Timestamptz(6)
  organization_id        Int?
  event_bookmarks        event_bookmarks[]
  event_tag              event_tag?
  addresses              addresses?               @relation(fields: [address_id], references: [id], onUpdate: NoAction)
  departments            departments?             @relation(fields: [department_id], references: [id], onUpdate: NoAction)
  users                  users?                   @relation(fields: [host_user_id], references: [id], onUpdate: NoAction)
  event_organization     event_organization?      @relation(fields: [organization_id], references: [id], onDelete: NoAction, onUpdate: NoAction, map: "organization_id")
  waste_event_statistics waste_event_statistics[]
  waste_logs_event       waste_logs_event[]

  @@index([start_at], map: "idx_events_start_at")
}

model facilities {
  id                 Int                      @id @default(autoincrement())
  name               String                   @db.VarChar(255)
  facility_type      String?                  @db.VarChar(100)
  address_id         Int?
  phone              String?                  @db.VarChar(20)
  location           Unsupported("geometry")?
  emergency_services Boolean?                 @default(false)
  department_id      Int?
  created_at         DateTime                 @default(now()) @db.Timestamptz(6)
  ambulances         ambulances[]
  appointments       appointments[]
  beds               beds[]
  doctors            doctors[]
  emergency_calls    emergency_calls[]
  addresses          addresses?               @relation(fields: [address_id], references: [id], onUpdate: NoAction)
  departments        departments?             @relation(fields: [department_id], references: [id], onUpdate: NoAction)
  medicine_inventory medicine_inventory[]
  payments           payments[]
  prescriptions      prescriptions[]
  staff              staff[]

  @@index([location], map: "idx_facilities_location", type: Gist)
}

model fact_healthcare {
  health_id                     Int           @id
  time_id                       Int?
  facility_id                   Int?
  avg_wait_time_minutes_numeric Decimal?      @db.Decimal(10, 2)
  bed_occupancy_percent_numeric Decimal?      @db.Decimal(5, 2)
  total_revenue_numeric         Decimal?      @db.Decimal(15, 2)
  dim_facility                  dim_facility? @relation(fields: [facility_id], references: [facility_id], onDelete: NoAction, onUpdate: NoAction, map: "fk_facility")
  dim_time                      dim_time?     @relation(fields: [time_id], references: [time_id], onDelete: NoAction, onUpdate: NoAction, map: "fk_time")
}

model fact_population {
  population_id              Int           @id
  time_id                    Int?
  location_id                Int?
  total_population           Int?
  population_density_numeric Decimal?      @db.Decimal(10, 2)
  median_age_numeric         Decimal?      @db.Decimal(5, 2)
  dim_location               dim_location? @relation(fields: [location_id], references: [location_id], onDelete: NoAction, onUpdate: NoAction, map: "fk_location")
  dim_time                   dim_time?     @relation(fields: [time_id], references: [time_id], onDelete: NoAction, onUpdate: NoAction, map: "fk_time")
}

model fact_traffic {
  traffic_id            Int           @id
  time_id               Int?
  location_id           Int?
  vehicle_count         Int?
  has_accident_flag     Boolean?
  density_level_numeric Decimal?      @db.Decimal(10, 2)
  dim_location          dim_location? @relation(fields: [location_id], references: [location_id], onDelete: NoAction, onUpdate: NoAction, map: "fk_location")
  dim_time              dim_time?     @relation(fields: [time_id], references: [time_id], onDelete: NoAction, onUpdate: NoAction, map: "fk_time")
}

model fact_waste {
  waste_id                     Int             @id
  time_id                      Int?
  location_id                  Int?
  waste_type_id                Int?
  collection_weight_kg_numeric Decimal?        @db.Decimal(10, 2)
  dim_location                 dim_location?   @relation(fields: [location_id], references: [location_id], onDelete: NoAction, onUpdate: NoAction, map: "fk_location")
  dim_time                     dim_time?       @relation(fields: [time_id], references: [time_id], onDelete: NoAction, onUpdate: NoAction, map: "fk_time")
  dim_waste_type               dim_waste_type? @relation(fields: [waste_type_id], references: [waste_type_id], onDelete: NoAction, onUpdate: NoAction, map: "fk_waste_type")
}

model fact_weather {
  weather_id              Int           @id
  time_id                 Int?
  location_id             Int?
  avg_aqi_numeric         Decimal?      @db.Decimal(10, 2)
  max_pm25_numeric        Decimal?      @db.Decimal(10, 2)
  avg_temperature_numeric Decimal?      @db.Decimal(5, 2)
  dim_location            dim_location? @relation(fields: [location_id], references: [location_id], onDelete: NoAction, onUpdate: NoAction, map: "fk_location")
  dim_time                dim_time?     @relation(fields: [time_id], references: [time_id], onDelete: NoAction, onUpdate: NoAction, map: "fk_time")
}

model favourite_district {
  id       Int    @id @default(autoincrement())
  user_id  Int
  district String @db.VarChar(255)
  users    users  @relation(fields: [user_id], references: [id], onDelete: NoAction, onUpdate: NoAction, map: "user_id")

  @@index([user_id], map: "fki_user_id")
}

model fcm_token {
  id         Int       @id @default(autoincrement())
  user_id    Int?
  created_at DateTime? @default(now()) @db.Timestamptz(6)
  updated_at DateTime? @default(now()) @db.Timestamptz(6)
  tokens     String
  users      users?    @relation(fields: [user_id], references: [id], onDelete: Cascade, onUpdate: NoAction)
}

model freecycle_categories {
  id                         Int                          @id @default(autoincrement())
  category_name              String                       @unique @db.VarChar(100)
  created_at                 DateTime                     @default(now()) @db.Timestamptz(6)
  updated_at                 DateTime                     @default(now()) @db.Timestamptz(6)
  freecycle_posts_categories freecycle_posts_categories[]
}

model freecycle_posts {
  id                         Int                          @id @default(autoincrement())
  item_name                  String                       @db.VarChar(255)
  item_weight                Decimal?                     @db.Decimal(10, 3)
  photo_url                  String?
  description                String?
  donater_id                 Int?
  donate_to_department_id    Int?
  is_given                   Boolean                      @default(false)
  created_at                 DateTime                     @default(now()) @db.Timestamptz(6)
  updated_at                 DateTime                     @default(now()) @db.Timestamptz(6)
  departments                departments?                 @relation(fields: [donate_to_department_id], references: [id], onUpdate: NoAction)
  users                      users?                       @relation(fields: [donater_id], references: [id], onUpdate: NoAction)
  freecycle_posts_categories freecycle_posts_categories[]
  receiver_requests          receiver_requests[]
}

model freecycle_posts_categories {
  post_id              Int
  category_id          Int
  freecycle_categories freecycle_categories @relation(fields: [category_id], references: [id], onDelete: Cascade, onUpdate: NoAction)
  freecycle_posts      freecycle_posts      @relation(fields: [post_id], references: [id], onDelete: Cascade, onUpdate: NoAction)

  @@id([post_id, category_id])
}

model insurance_cards {
  id          Int            @id @default(autoincrement())
  user_id     Int?
  balance     Decimal?       @default(0) @db.Decimal(14, 2)
  card_number String?        @unique @db.VarChar(50)
  status      wallet_status? @default(active)
  created_at  DateTime       @default(now()) @db.Timestamptz(6)
  updated_at  DateTime       @default(now()) @db.Timestamptz(6)
  users       users?         @relation(fields: [user_id], references: [id], onDelete: Cascade, onUpdate: NoAction)
}

model intersections {
  id                                               Int                      @id @default(autoincrement())
  location                                         Unsupported("geometry")?
  roads_roads_end_intersection_idTointersections   roads[]                  @relation("roads_end_intersection_idTointersections")
  roads_roads_start_intersection_idTointersections roads[]                  @relation("roads_start_intersection_idTointersections")
  traffic_lights                                   traffic_lights[]
}

model light_requests {
  id               Int             @id @default(autoincrement())
  traffic_light_id Int?
  requested_at     DateTime        @default(now()) @db.Timestamptz(6)
  traffic_lights   traffic_lights? @relation(fields: [traffic_light_id], references: [id], onDelete: Cascade, onUpdate: NoAction)
}

model marker {
  id             Int                      @id @default(autoincrement())
  marker_type_id Int?
  description    String?
  location       Unsupported("geometry")?
  created_at     DateTime                 @default(now()) @db.Timestamptz(6)
  updated_at     DateTime                 @default(now()) @db.Timestamptz(6)
  marker_type    marker_type?             @relation(fields: [marker_type_id], references: [id], onDelete: Cascade, onUpdate: NoAction)
}

model marker_type {
  id                Int      @id @default(autoincrement())
  marker_type_icon  String?  @db.VarChar(255)
  created_at        DateTime @default(now()) @db.Timestamptz(6)
  updated_at        DateTime @default(now()) @db.Timestamptz(6)
  marker_type_color String?  @db.VarChar(255)
  marker            marker[]
}

model medicine_inventory {
  id             Int         @id @default(autoincrement())
  facility_id    Int?
  medicine_name  String?     @db.VarChar(255)
  stock_quantity Int?        @default(0)
  unit_price     Decimal?    @default(0) @db.Decimal(12, 2)
